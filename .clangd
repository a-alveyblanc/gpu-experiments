CompileFlags:
  Compiler: /opt/cuda/bin/nvcc
  Add:
    - --cuda-path=/opt/cuda/
    - --cuda-gpu-arch=sm_86
    - -I/home/aj/tools/cutlass/include/
    - -I/home/aj/tools/cutlass/tools/util/include/
    - -I/home/aj/tools/cutlass/cutlass/examples/common/
    - -I/opt/cuda/targets/x86_64-linux/include/cccl/
    - -I/usr/include/
    - "-xcuda"
    - "-ferror-limit=0"
    - --std=c++17
    - "-D__CUDA_ARCH__=860"
    - "-Wno-invalid-constexpr"
  Remove:
    - "-Xfatbin*"
    - "-gencode*"
    - "--generate-code*"
    - "-ccbin*"
    - "--compiler-options*"
    - "--expt-extended-lambda"
    - "--expt-relaxed-constexpr"
    - "-forward-unknown-to-host-compiler"
    - "-Werror=cross-execution-space-call"
Hover:
  ShowAKA: No
InlayHints:
  Enabled: No
Diagnostics:
  Suppress:
    - "variadic_device_fn"
    - "attributes_not_allowed"
